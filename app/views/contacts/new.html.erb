<div class="container">
  <h1 class="text-centerrails ">New contact form</h1>
  
  <%= form_for "/contacts" do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    
    <%= f.label :email %>
    <%= f.text_field :name %>
    
    <%= f.label :comments %>
    <%= f.text_area :comments %>
    
    <%= f.submit 'Submit' %>
  <% end %>
</div>